<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
					xmlns:tools="http://schemas.android.com/tools"
					package="com.zlate87.fyberapp">

	<application
			android:allowBackup="false"
			android:name="com.zlate87.fyberapp.base.App"
			android:icon="@mipmap/ic_launcher"
			android:label="@string/app_name"
			android:supportsRtl="true"
			android:theme="@style/AppTheme"
			tools:ignore="GoogleAppIndexingWarning">
		<activity android:name="com.zlate87.fyberapp.feature.form.controller.FormActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity
				android:name="com.zlate87.fyberapp.feature.offers.controller.OffersActivity"
				android:label="@string/title_activity_offers">
			<meta-data
					android:name="android.support.PARENT_ACTIVITY"
					android:value="com.zlate87.fyberapp.feature.form.controller.FormActivity" />
		</activity>
	</application>

</manifest>
